<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ProjectServlet</servlet-name>
  	<display-name>ProjectServlet</display-name>
  	<description></description>
  	<servlet-class>com.klef.Servlet.ProjectServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListProjectsServlet</servlet-name>
  	<display-name>ListProjectsServlet</display-name>
  	<description></description>
  	<servlet-class>com.klef.Servlet.ListProjectsServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>ProjectServlet</servlet-name>
  	<url-pattern>/ProjectServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListProjectsServlet</servlet-name>
  	<url-pattern>/ListProjectsServlet</url-pattern>
  </servlet-mapping>

 
</web-app>
